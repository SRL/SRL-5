function OpenBankGlass(WhichBank: string; ChangeCompass, ChangeAngle: Boolean): Boolean;
var
  OBC: TPoint;
  c, Speed, Col: Integer;
begin
  result := false;
  Speed := GetColorToleranceSpeed;
